# MongoDB Configuration
# Connection string for MongoDB database
MONGODB_CONNECTION_STRING=mongodb://mongodb:mongodb@mongodb_session_manager-mongodb:27017/

# Database and collection names
DATABASE_NAME=examples
COLLECTION_NAME=sessions

# MongoDB Connection Pool Settings
MAX_POOL_SIZE=100
MIN_POOL_SIZE=10
MAX_IDLE_TIME_MS=30000

# Backend Server Configuration
# Host and port for the FastAPI server
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8882

# CORS Configuration
# Frontend URL for CORS
FRONTEND_URL=http://localhost:8883

# Allowed origins (comma-separated)
ALLOWED_ORIGINS_STR=http://localhost:8883,http://127.0.0.1:8883

# Pagination Settings
# Default number of results per page
DEFAULT_PAGE_SIZE=20

# Maximum number of results per page
MAX_PAGE_SIZE=100

# Logging Configuration
# Log level: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Authentication
# Password for accessing the application
BACKEND_PASSWORD=123456
